		"chalk": "1.x",
		"image-size": "0.x",
		"limiter": "1.x",
		"load-class": "1.x",
		"markdown-it": "8.x",
		"mime-types": "2.x",
		"mkdirp": "0.x",
		"node-cache": "4.x",
		"node-watch": "0.x",
		"pem": "1.x",
		"pug": "2.0.0-rc.4",
		"request": ">=2.81.0",
		"spdy": "3.x",
		"strict-mode": "*",

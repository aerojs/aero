{
	"name": "aero",
	"version": "2.1.0",
	"description": "The fastest node.js framework.",
	"repository": "aerojs/aero",
	"homepage": "https://github.com/aerojs/aero",
	"license": "GPL-3.0",
	"main": "lib/index.js",
	"dependencies": {
		"autoprefixer-stylus": "0.x",
		"bluebird": "3.x",
